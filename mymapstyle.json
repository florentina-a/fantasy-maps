    {
      "id": "urban",
      "type": "fill",
      "source": "r68scq4",
      "filter": ["all", ["==", "terrain_type", "urban"]],
      "paint": {
        "fill-opacity": 0.9,
        "fill-color": "#78909C",
        "fill-outline-color": "#546E7A"
