{
  "version": 8,
  "name": "MyMapStyle",
  "metadata": {"maputnik:renderer": "mlgljs"},
    "sources": {
    "background-image": {
      "type": "image",
      "url": "https://raw.githubusercontent.com/florentina-a/fantasy-maps/main/ChatGPT4o_MyFictionalWorldMap.png",
      "coordinates": [
  	[-180, -85], 
  	[180, -85],   
  	[180, 85],   
  	[-180, 85]
	]
    },
    "r68scq4": {
      "type": "geojson",
      "data": "https://raw.githubusercontent.com/florentina-a/fantasy-maps/main/map.geojson"
    }
  },
  "sprite": "",
