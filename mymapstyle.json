        "fill-opacity": 0.9,
        "fill-outline-color": "#5D4037"
      }
    },
    {
      "id": "forest",
      "type": "fill",
      "source": "r68scq4",
      "filter": ["all", ["==", "terrain_type", "forest"]],
      "paint": {
        "fill-color": "#2E7D32",
        "fill-opacity": 0.85,
        "fill-outline-color": "#1B5E20"
      }
    },
    {
      "id": "hill",
      "type": "fill",
      "source": "r68scq4",
      "filter": ["all", ["==", "terrain_type", "hill"]],
      "paint": {
        "fill-color": "#A1887F",
        "fill-outline-color": "#8D6E63",
        "fill-opacity": 0.8
      }
    },
    {
      "id": "sea",
      "type": "fill",
      "source": "r68scq4",
      "filter": ["all", ["==", "terrain_type", "sea"]],
      "paint": {
        "fill-opacity": 0.75,
        "fill-color": "#1E88E5",
        "fill-outline-color": "#1565C0"
